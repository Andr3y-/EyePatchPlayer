CLANG_CXX_LANGUAGE_STANDARD = compiler-default
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AFNetworking.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RESideMenu.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RSPlayPauseButton.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Realm.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SDWebImage.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/StreamingKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/VK_ios_sdk.framework/Headers"
OTHER_CPLUSPLUSFLAGS = -std=c++1y $(inherited)
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AFNetworking" -framework "RESideMenu" -framework "RSPlayPauseButton" -framework "Realm" -framework "SDWebImage" -framework "StreamingKit" -framework "VK_ios_sdk"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods