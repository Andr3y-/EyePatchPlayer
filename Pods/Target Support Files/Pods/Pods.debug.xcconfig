CLANG_CXX_LANGUAGE_STANDARD = compiler-default
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Crashlytics" "$(PODS_ROOT)/Fabric"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Crashlytics" "${PODS_ROOT}/Headers/Public/Crashlytics/Crashlytics" "${PODS_ROOT}/Headers/Public/Fabric" "${PODS_ROOT}/Headers/Public/Fabric/Fabric"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AFNetworking.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/DGActivityIndicatorView.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RESideMenu.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RSPlayPauseButton.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Realm.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SDWebImage.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/StreamingKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/VK_ios_sdk.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Crashlytics" -isystem "${PODS_ROOT}/Headers/Public/Crashlytics/Crashlytics" -isystem "${PODS_ROOT}/Headers/Public/Fabric" -isystem "${PODS_ROOT}/Headers/Public/Fabric/Fabric"
OTHER_CPLUSPLUSFLAGS = -std=c++1y $(inherited)
OTHER_LDFLAGS = $(inherited) -ObjC -l"c++" -l"z" -framework "AFNetworking" -framework "Crashlytics" -framework "DGActivityIndicatorView" -framework "Fabric" -framework "RESideMenu" -framework "RSPlayPauseButton" -framework "Realm" -framework "SDWebImage" -framework "Security" -framework "StreamingKit" -framework "SystemConfiguration" -framework "UIKit" -framework "VK_ios_sdk"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods